; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="The Sparkling Farce"
config/description="A tactical RPG platform inspired by Shining Force"
config/version="0.1.0"
run/main_scene="res://mods/_sandbox/scenes/test_unit.tscn"
config/features=PackedStringArray("4.5", "Forward Plus")

[autoload]

ModLoader="*res://core/mod_system/mod_loader.gd"
GridManager="*res://core/systems/grid_manager.gd"
TurnManager="*res://core/systems/turn_manager.gd"
InputManager="*res://core/systems/input_manager.gd"
BattleManager="*res://core/systems/battle_manager.gd"
AIController="*res://core/systems/ai_controller.gd"

[debug]

gdscript/warnings/untyped_declaration=2
gdscript/warnings/unsafe_property_access=1
gdscript/warnings/unsafe_method_access=1
gdscript/warnings/unsafe_cast=1
gdscript/warnings/unsafe_call_argument=1
gdscript/warnings/infer_on_variant=2

[display]

window/size/viewport_width=640
window/size/viewport_height=360
window/size/window_width_override=1280
window/size/window_height_override=720
window/stretch/mode="viewport"
window/stretch/aspect="ignore"

[editor]

version_control/plugin_name="GitPlugin"
version_control/autoload_on_startup=true

[editor_plugins]

enabled=PackedStringArray("res://addons/sparkling_editor/plugin.cfg")

[file_customization]

folder_colors={
"res://addons/": "purple",
"res://assets/": "blue",
"res://core/": "red",
"res://data/": "green",
"res://mods/": "teal",
"res://scenes/": "pink",
"res://templates/": "yellow",
"res://user_content/": "orange"
}

